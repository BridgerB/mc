# Personal configuration
# Copy this file to config.nix and fill in your own values
{
  # SSH public keys for root user access
  # Add your SSH public keys here (from ~/.ssh/id_ed25519.pub or similar)
  sshKeys = [
    "ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAI... your-username@hostname"
    # You can add multiple keys
  ];
}
